{
  "name": "tree-helper",
  "version": "1.0.0",
  "description": "a javascript utils for tree",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "clear": "if [ -r dist/ ];then rm -r dist/;fi",
    "build": "clear && rollup --config",
    "test": "pnpm run build && jest",
    "release": "pnpm run build && np --no-2fa --branch main --message v%s",
    "release:test": "pnpm run build && np --no-2fa --branch main --no-publish"
  },
  "keywords": [
    "typescript",
    "tree"
  ],
  "author": "wangqs",
  "license": "ISC",
  "publishConfig": {
    "registry": "https://www.npmjs.com/~haibianlucky"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "jest": "^29.4.3",
    "np": "^7.6.3",
    "rollup": "^3.18.0",
    "rollup-plugin-babel": "^4.4.0"
  }
}
